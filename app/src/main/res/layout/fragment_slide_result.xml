<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="@color/toolbarTransparent"
    android:clipToPadding="false"
    >


    <com.makeramen.roundedimageview.RoundedImageView
        android:id="@+id/imageDetail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:riv_corner_radius_bottom_left="@dimen/padding_xxx"
        app:riv_corner_radius_bottom_right="@dimen/padding_xxx"
        app:riv_corner_radius_top_left="@dimen/padding_xxx"
        app:riv_corner_radius_top_right="@dimen/padding_xxx"
        android:scaleType="fitXY"
        android:src="@drawable/loading_animation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:background="@drawable/sum" />
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/ic_slow_motion_video_black_24dp"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

